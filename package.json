{
  "babel": {
    "presets": [
      "es2015-node5"
    ],
    "plugins": [
      "transform-object-rest-spread",
      [
        "transform-async-to-module-method",
        {
          "module": "bluebird",
          "method": "coroutine"
        }
      ]
    ]
  },
  "dependencies": {
    "babel-eslint": "^6.0.0",
    "babel-plugin-transform-async-to-module-method": "^6.7.0",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-es2015-node5": "^1.2.0",
    "babel-register": "^6.7.2",
    "bluebird": "^3.3.4",
    "bower": "^1.7.7",
    "cheerio": "^0.20.0",
    "dotenv": "^2.0.0",
    "express": "^4.13.4",
    "isomorphic-fetch": "^2.2.1",
    "jade": "^1.11.0",
    "koa": "^2.0.0-alpha.3",
    "koa-router": "^7.0.0",
    "marked": "^0.3.5",
    "node-sass": "^3.4.2"
  },
  "devDependencies": {
    "eslint": "^2.5.3",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-babel": "^3.1.0",
    "nodemon": "^1.9.1"
  },
  "engines": {
    "node": ">=5 <6"
  },
  "private": "true",
  "scripts": {
    "postinstall": "bower install && npm run styles",
    "styles": "node-sass --output-style compressed --include-path bower_components --output public/ ./sass/",
    "styles:dev": "node-sass --include-path bower_components --watch --output public/ ./sass/",
    "start:dev": "nodemon --ext js,jade,css",
    "test": "eslint ./server"
  }
}
